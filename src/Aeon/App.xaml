<Application
    x:Class="Aeon.Emulator.Launcher.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="EmulatorDisplayResources.xaml" />
                <ResourceDictionary Source="RoundButtonResources.xaml" />
                <ResourceDictionary>
                    <LinearGradientBrush x:Key="backgroundGradient">
                        <GradientStop Offset="0" Color="SteelBlue" />
                        <GradientStop Offset="1" Color="#FF1D4461" />
                    </LinearGradientBrush>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
