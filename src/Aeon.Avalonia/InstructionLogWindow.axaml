<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Aeon.Emulator.Launcher.InstructionLogWindow"
        Title="Instruction Log" Height="450" Width="800">
	<DockPanel>
		<StackPanel Orientation="Horizontal" DockPanel.Dock="Top" Background="Transparent">
			<TextBox Name="gotoAddressBox" Width="100" />
			<Button Content="Next" Name="NextButton" />
			<Button Content="Prev" />
			<Separator />
			<Button Content="Next V86 Change" Click="NextV86_Click" />
			<Button Content="Last V86 Change" Click="LastV86_Click" />
			<Separator />
			<Button Content="Find Next Error" Click="FindNextError_Click" />
		</StackPanel>
		<Grid>
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="200" />
				<ColumnDefinition />
			</Grid.ColumnDefinitions>
			<TextBlock Name="registerText" FontFamily="Consolas" HorizontalAlignment="Left" VerticalAlignment="Top" />
			<ListBox Grid.Column="1" Name="historyList" FontFamily="Consolas"
					 SelectionChanged="HistoryList_SelectionChanged" />
		</Grid>
	</DockPanel>
</Window>
